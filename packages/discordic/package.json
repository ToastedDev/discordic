{
  "name": "discordic",
  "description": "The Discord package of your dreams.",
  "version": "0.0.0",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://discordic.js.org",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsx src/test.ts",
    "build": "rm -rf dist && tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc"
  },
  "dependencies": {
    "ws": "^8.14.1"
  },
  "devDependencies": {
    "@types/ws": "^8.5.5",
    "dotenv": "^16.3.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.10"
  }
}
